        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "x=4\n",
        "x1=np.array([x])\n",
        "n=100\n",
        "a=5\n",
        "c=0\n",
        "m=7\n",
        "for i in range(n):\n",
        "    x=(a*x+c)%m\n",
        "    x1=np.append(x1,x)\n",
        "print(x1)\n",
        "ind=np.arange(n+1)\n",
        "plt.bar(ind, x1)\n",
        "plt.show()"
      ]
